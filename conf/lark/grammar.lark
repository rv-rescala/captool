?start: statement+

?statement: open_call
    | click_by_class
    | move_by_class
    | capture_by_screen

open_call: "open"
click_by_class: "click_by_class" "(" string ")"
move_by_class: "move_by_class" "(" string ")"
capture_by_screen: "capture_by_screen"


string    : ESCAPED_STRING
symbol    : WORD

%import common.ESCAPED_STRING
%import common.WORD
%import common.WS
%ignore WS